<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Cirkel 1 -->
    <item android:top="10dp" android:left="10dp" android:right="10dp" android:bottom="10dp">
        <shape android:shape="oval">
            <solid android:color="#FF5733" /> <!-- Färg för cirkel 1 -->
            <size android:width="40dp" android:height="40dp" /> <!-- Storlek på cirkel 1 -->
        </shape>
    </item>

    <!-- Cirkel 2 -->
    <item android:top="10dp" android:left="10dp" android:right="10dp" android:bottom="10dp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="oval">
                <solid android:color="#FFFFFF" /> <!-- Färg för cirkel 2 -->
                <size android:width="40dp" android:height="40dp" /> <!-- Storlek på cirkel 2 -->
            </shape>
        </rotate>
    </item>
</layer-list>